{
  "private": true,
  "source": "public/index.html",
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src/WebApp -s -o fable & parcel",
    "build": "dotnet fable src/WebApp -s -o fable --run parcel build --public-url \"./\"",
    "bundle-report": "parcel build --public-url \"./\" --reporter @parcel/reporter-bundle-analyzer"
  },
  "dependencies": {
    "preact": "^10.11.2",
    "react-pure-modal": "^2.2.0",
    "react-select": "^5.6.0",
    "recharts": "^2.1.16",
    "yalps": "^0.3.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.0",
    "@parcel/reporter-bundle-analyzer": "^2.8.0",
    "@parcel/transformer-sass": "^2.8.0",
    "parcel": "^2.8.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "process": "^0.11.10"
  },
  "staticFiles": {
    "staticPath": "public/static"
  }
}
