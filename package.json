{
  "private": true,
  "source": [
    "public/index.html",
    "public/info.html",
    "public/credits.html"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "scripts": {
    "postinstall": "dotnet tool restore && dotnet paket install",
    "watch": "dotnet fable watch src/WebApp --noCache -s -o fable",
    "start": "parcel",
    "build:fable": "dotnet fable src/WebApp -s -o fable",
    "build": "pnpm build:fable && parcel build --public-url './'",
    "bundle-report": "pnpm build:fable && parcel build --public-url './' --reporter @parcel/reporter-bundle-analyzer"
  },
  "dependencies": {
    "preact": "^10.11.3",
    "recharts": "^2.2.0",
    "yalps": "^0.3.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.2",
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "parcel": "^2.8.2",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "process": "^0.11.10"
  },
  "staticFiles": {
    "staticPath": "public/static"
  }
}
